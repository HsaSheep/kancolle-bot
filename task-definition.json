{
    "containerDefinitions": [
        {
            "name": "kancolle-bot",
            "image": "kancolle-bot",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "kancolle-bot-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "secrets": [
                {
                    "name": "Fubuki_TOKEN",
                    "valueFrom": "Fubuki_TOKEN"
                },
                {
                    "name": "Kongou_TOKEN",
                    "valueFrom": "Kongou_TOKEN"
                },
                {
                    "name": "textChannelId",
                    "valueFrom": "textChannelId"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "valueFrom": "OPENAI_API_KEY"
                },
                {
                    "name": "S3_BUCKET_NAME",
                    "valueFrom": "S3_BUCKET_NAME"
                }
            ],
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/kancolle-bot-DEFINITION",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "HEALTHCHECK CMD discordhealthcheck || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3
            }
        }
    ],
    "family": "kancolle-bot-DEFINITION",
    "taskRoleArn": "arn:aws:iam::921464148674:role/Kancolle-Task-Role",
    "executionRoleArn": "arn:aws:iam::921464148674:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}